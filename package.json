{
  "name": "ewell",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "cross-env REACT_APP_PREFIX=ewell craco start",
    "dev:vconsole": "cross-env REACT_APP_CONSOLE=vconsole npm start",
